jobs
{
    {
        @inherit                                 ../../common.serge#job_template

        source_match                             ^messages\.json$

        parser
        {
            plugin                               parse_chrome_json
        }

        serializer
        {
            plugin                               serialize_xliff2

            data {
                context_strategy                 name
            }
        }

        ts_file_path                             ./test-output/po/%LOCALE%/%FILE%.xliff
    }
}
